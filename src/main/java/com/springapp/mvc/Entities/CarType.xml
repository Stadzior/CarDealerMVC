<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.springapp.mvc.Entities.DiccartypesEntity" table="diccartypes" schema="" catalog="usedcarsdealer">
        <id name="idCarType" column="IdCarType"/>
        <property name="nameCarType" column="NameCarType"/>
        <set name="logbooksesByIdCarType" inverse="true">
            <key>
                <column name="IdCarType"/>
            </key>
            <one-to-many not-found="ignore" class="com.springapp.mvc.Entities.LogbooksEntity"/>
        </set>
    </class>
</hibernate-mapping>