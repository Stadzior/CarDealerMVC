<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.springapp.mvc.Entities.DicbodytypesEntity" table="dicbodytypes" schema="" catalog="usedcarsdealer">
        <id name="idBodyType" column="IdBodyType"/>
        <property name="nameBodyType" column="NameBodyType"/>
        <set name="logbooksesByIdBodyType" inverse="true">
            <key>
                <column name="IdBodyType"/>
            </key>
            <one-to-many not-found="ignore" class="com.springapp.mvc.Entities.LogbooksEntity"/>
        </set>
    </class>
</hibernate-mapping>