<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mappedClasses.DicbrandsEntity" table="dicbrands" schema="" catalog="usedcarsdealer">
        <id name="idBrand" column="IdBrand"/>
        <property name="nameBrand" column="NameBrand"/>
        <set name="modelsesByIdBrand" inverse="true">
            <key>
                <column name="IdBrand"/>
            </key>
            <one-to-many not-found="ignore" class="mappedClasses.ModelsEntity"/>
        </set>
        <set name="offersesByIdBrand" inverse="true">
            <key>
                <column name="IdBrand"/>
            </key>
            <one-to-many not-found="ignore" class="mappedClasses.OffersEntity"/>
        </set>
    </class>
</hibernate-mapping>