<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mappedClasses.DicbodycolorsEntity" table="dicbodycolors" schema="" catalog="usedcarsdealer">
        <id name="idBodyColor" column="IdBodyColor"/>
        <property name="nameBodyColor" column="NameBodyColor"/>
        <set name="logbooksesByIdBodyColor" inverse="true">
            <key>
                <column name="IdBodyColor"/>
            </key>
            <one-to-many not-found="ignore" class="mappedClasses.LogbooksEntity"/>
        </set>
    </class>
</hibernate-mapping>