<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mappedClasses.DicgearboxtypesEntity" table="dicgearboxtypes" schema="" catalog="usedcarsdealer">
        <id name="idGearBoxType" column="IdGearBoxType"/>
        <property name="nameGearBoxType" column="NameGearBoxType"/>
        <set name="gearboxesesByIdGearBoxType" inverse="true">
            <key>
                <column name="IdGearBoxType"/>
            </key>
            <one-to-many not-found="ignore" class="mappedClasses.GearboxesEntity"/>
        </set>
    </class>
</hibernate-mapping>