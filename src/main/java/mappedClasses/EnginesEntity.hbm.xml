<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mappedClasses.EnginesEntity" table="engines" schema="" catalog="usedcarsdealer">
        <id name="idEngine" column="IdEngine"/>
        <property name="idFuelType" column="IdFuelType"/>
        <property name="volumeEngine" column="VolumeEngine"/>
        <property name="horsePower" column="HorsePower"/>
        <property name="description" column="Description"/>
        <many-to-one name="dicfueltypesByIdFuelType" class="mappedClasses.DicfueltypesEntity">
            <column name="IdFuelType"/>
        </many-to-one>
        <set name="logbooksesByIdEngine" inverse="true">
            <key>
                <column name="IdEngine"/>
            </key>
            <one-to-many not-found="ignore" class="mappedClasses.LogbooksEntity"/>
        </set>
    </class>
</hibernate-mapping>