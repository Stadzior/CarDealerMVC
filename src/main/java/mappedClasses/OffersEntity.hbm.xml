<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="mappedClasses.OffersEntity" table="offers" schema="" catalog="usedcarsdealer">
        <id name="idOffer" column="IdOffer"/>
        <property name="price" column="Price"/>
        <property name="yearOfProduction" column="YearOfProduction"/>
        <property name="idFuelType" column="IdFuelType"/>
        <property name="idBrand" column="IdBrand"/>
        <property name="idState" column="IdState"/>
        <property name="distanceDriven" column="DistanceDriven"/>
        <property name="phoneNumber" column="PhoneNumber"/>
        <property name="description" column="Description"/>
        <many-to-one name="dicbrandsByIdBrand" class="mappedClasses.DicbrandsEntity">
            <column name="IdBrand"/>
        </many-to-one>
        <many-to-one name="dicfueltypesByIdFuelType" class="mappedClasses.DicfueltypesEntity">
            <column name="IdFuelType"/>
        </many-to-one>
        <many-to-one name="dicstatesByIdState" class="mappedClasses.DicstatesEntity">
            <column name="IdState"/>
        </many-to-one>
    </class>
</hibernate-mapping>